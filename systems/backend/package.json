    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=14",
    "yarn": "Use npm"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/**/*.ts",
      "!<rootDir>/**/*.spec.ts",
      "!<rootDir>/migrations/*.ts"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "globalSetup": "<rootDir>/test-helpers/jest/e2e-global-setup.js",
    "injectGlobals": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "<rootDir>/test-helpers/jest/e2e-test-environment.js",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "license": "UNLICENSED",
  "name": "backend",
  "private": true,
  "scripts": {
    "build": "nest build",
    "eslint": "eslint --ext=json,ts,yml",
    "lint": "npx eslint --ext=json,ts,yml --fix .",
    "lint:ci": "npx eslint --ext=json,ts,yml .",
    "migration:create": "npx typeorm migration:create --config ./ormconfig.cli.js",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.http.js",
    "test": "jest --maxWorkers=50%",
    "test:ci": "APP_ENV=test NODE_ENV=test Ci=true jest --coverage --ci --runInBand --bail"
  },
  "version": "1.2.0"
}
