    "@types/node": "17.0.23",
    "@types/passport-jwt": "3.0.6",
    "@types/ramda": "0.28.4",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "eslint": "8.12.0",
    "graphql-tag": "2.12.6",
    "jest": "27.5.1",
    "jest-environment-node": "27.5.1",
    "supertest": "6.2.2",
